<Window x:Class="KeepYourTime.ViewWindows.MessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MessageWindow" Height="150" Width="310"
        WindowStartupLocation="Manual">
    <Grid>
        <!--<Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>-->
        <StackPanel >
            <TextBlock Name="txtResult" Text="{Binding Status}"/>
            <TextBlock Name="txtMensagem" Text="{Binding Message}" />
            <TextBlock Name="txtQuery" Text="{Binding Query}" />
            <TextBlock Name="txtDetalhe" Text="{Binding StackTrace}"/>
        </StackPanel>
    </Grid>
</Window>
